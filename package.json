{
  "name": "idor",
  "version": "1.0.0",
  "description": "A type-based ID obfuscation library to prevent insecure direct object references (IDOR)",
  "homepage": "https://github.com/ttoohey/idor",
  "repository": {
    "type": "git",
    "url": "https://github.com/ttoohey/idor.git"
  },
  "main": "dist",
  "files": [
    "dist/*.js",
    "dist/**/*.js",
    "**/package.json"
  ],
  "scripts": {
    "build": "babel -d dist src",
    "clean": "rm -rf dist",
    "lint": "eslint --ext .js --ext .json src",
    "prepare": "npm run clean && npm run build",
    "repl": "babel-node node_modules/.bin/local-repl",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "idor",
    "globalid",
    "relayid",
    "graphql",
    "objectionjs"
  ],
  "author": "Gency Digital <hello@gency.com.au>",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/preset-env": "^7.3.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.14.1",
    "eslint-config-prettier": "^4.0.0",
    "local-repl": "^4.0.0",
    "prettier": "^1.16.4"
  },
  "optionalDependencies": {
    "graphql": "^14.1.1",
    "lodash": "^4.17.11"
  }
}
